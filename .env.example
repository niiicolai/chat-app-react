# VITE_DEBUG
# Enable debug mode
# Example: true
VITE_DEBUG=true

# VITE_CLIENT_URL
# URL of the client application
# Example: http://localhost:5173
# Example: https://chat-client.example.com
VITE_CLIENT_URL=http://localhost:5173

# VITE_API_URL
# URL of the API server
# Example: http://localhost:3000
# Example: https://chat-api.example.com
VITE_API_URL=http://localhost:3000

# VITE_WS_URL
# URL of the WebSocket server
# Example: ws://localhost:3001
# Example: wss://chat-ws.example.com
VITE_WS_URL=ws://localhost:3001

# VITE_API_PREFIX
# Prefix for the API routes
# Example: /api/v1
VITE_API_PREFIX=/api/v1

# VITE_AUTH_LS_KEY
# Local storage key for the authentication token
# Example: chat-auth-token
VITE_AUTH_LS_KEY=chat-auth-token